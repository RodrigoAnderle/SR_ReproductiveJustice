@article{ WOS:000685845500004,
Author = {Ayubi, Zahra},
Title = {AUTHORITY AND EPISTEMOLOGY IN ISLAMIC MEDICAL ETHICS OF WOMEN'S
   REPRODUCTIVE HEALTH},
Journal = {JOURNAL OF RELIGIOUS ETHICS},
Year = {2021},
Volume = {49},
Number = {2},
Pages = {245-269},
Month = {JUN},
Abstract = {There is a serious lacuna in Islamic medical ethics in considering the
   category of gender, especially in the consciousness that gender,
   maleness, femaleness, or non-binary status, affects how patients receive
   medical counsel and medical care, and interact with religious
   authorities and care providers. This lack of attention to gender is also
   true for specifically so-called ``women's health{''} topics such as
   assisted reproductive technologies or abortion. This essay argues that
   in order to be a more ethical discourse, Islamic medical ethics of
   reproductive health needs to break free from the gendered limitations of
   male-only authority of jurists and the limited legal frameworks offered
   by the fiqh genre. Instead it ought to incorporate multiple
   epistemologies of Islamic ethics.},
