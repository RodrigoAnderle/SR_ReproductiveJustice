@article{ WOS:000626192600001,
Author = {Guerra-Reyes, Lucia and Palacios, Iris and Ferstead, Amanda},
Title = {Managing Precarity: Understanding Latinas' Sexual and Reproductive
   Care-Seeking in a Midwest Emergent Latino Community},
Journal = {QUALITATIVE HEALTH RESEARCH},
