@article{ WOS:000905247500001,
Author = {Huslage, Melody and Ely, Gretchen E. and Nugent, William R. and
   Auerbach, Samantha and Agbemenu, Kafuli},
Title = {Reproductive Autonomy in Appalachia: An Investigation into Perceived
   Contraceptive Pressure},
Journal = {JOURNAL OF INTERPERSONAL VIOLENCE},
Year = {2023},
Volume = {38},
Number = {9-10},
Pages = {6985-7011},
Month = {MAY},
Abstract = {The reproductive autonomy of persons who can give birth can be impeded
   through forms of interpersonal violence and coercion. Moreover,
   macro-level factors (e.g., poverty, discrimination, community violence,
   legislative policies) may impede the reproductive autonomy of entire
   communities. This study investigates a form of violence we term
   perceived contraceptive pressure in Appalachia, an understudied region
   of the Eastern U.S., regarding reproductive health and decision-making.
   Through targeted Meta advertising, participants (N = 632) residing in
   Appalachian zip codes completed an online survey on reproductive health.
   The focus of this study was to investigate the prevalence of perceived
   contraceptive pressure, who was at increased risk of experiencing
   pressure, and the source(s) of perceived pressure. Binomial regressions
   were conducted on three different dependent variables: perceived
   pressure to be sterilized, perceived pressure to use birth control, and
   perceived pressure not to use birth control. Approximately half of all
   respondents (49.5\%) reported experiencing at least one type of pressure
   targeting contraceptive decision-making. The most prevalent source of
   perceived pressure to use birth control was from the healthcare provider
   (67.4\%), and the most prevalent source of perceived pressure not to use
   birth control was the respondent's partner (51.1\%). Recommendations for
   providers serving clients in the Appalachian region include pursuing
   education regarding contraceptive pressure at the individual level and
   macro-level. In addition, Appalachian residents may benefit from
   educational programming on reproductive autonomy, healthy relationships,
   and how to navigate pressure in relationships.},
