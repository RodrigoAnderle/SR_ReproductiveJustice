@article{ WOS:000438891200005,
Author = {Malvern, Chiweshe and Macleod, Catriona},
Title = {Cultural De-colonization versus Liberal Approaches to Abortion in
   Africa: The Politics of Representation and Voice},
Journal = {AFRICAN JOURNAL OF REPRODUCTIVE HEALTH},
