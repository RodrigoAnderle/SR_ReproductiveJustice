@article{ WOS:000219791600009,
Author = {Shaw, Jesica},
Title = {Full-Spectrum Reproductive Justice: The Affinity of Abortion Rights and
   Birth Activism},
Journal = {STUDIES IN SOCIAL JUSTICE},
Year = {2013},
Volume = {7},
Number = {1},
Pages = {143-159},
Abstract = {This paper argues that not only is there a relationship between birth
   activism and abortion activism, but that if empowering women is the
   goal, the two cannot be separated. By understanding how women's bodies
   have been controlled and their reproductive lives appropriated, the
   current pro-choice and birth activist frameworks that are used to
   advocate for women can no longer be understood to address women's needs.
   It is by working through the framework of full-spectrum reproductive
   justice that women may become truly empowered to regain control over
   their reproductive lives.},
