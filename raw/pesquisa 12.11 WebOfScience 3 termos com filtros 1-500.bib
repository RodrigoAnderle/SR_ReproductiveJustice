@article{ WOS:001054528500005,
Author = {Becker, Andrea},
Title = {STRATIFIED REPRODUCTION, HYSTERECTOMY, AND THE SOCIAL PROCESS OF OPTING
   INTO INFERTILITY},
Journal = {GENDER \& SOCIETY},
