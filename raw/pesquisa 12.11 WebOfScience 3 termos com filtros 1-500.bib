@article{ WOS:000869844100010,
Author = {Berrahou, Iman K. and Grimes, Andrew and Autry, Amy M. and Hawkins,
   Mitzi},
Title = {Management of Menstruation in Transgender and Gender Nonbinary
   Adolescents},
Journal = {CLINICAL OBSTETRICS AND GYNECOLOGY},
