@article{ WOS:000870756400001,
Author = {Kivlighan, Katie T. and Gardner, Tamara and Murphy, Carrie and Reiss,
   Paula and Griffin, Carrie and Migliaccio, Laura},
Title = {Grounded in Community: Development of a Birth Justice-Focused Volunteer
   Birth Companion Program},
