@article{ WOS:000975390900003,
Author = {Roemer, Neoshia R.},
Title = {THE INDIAN CHILD WELFARE ACT AS REPRODUCTIVE JUSTICE},
Journal = {BOSTON UNIVERSITY LAW REVIEW},
