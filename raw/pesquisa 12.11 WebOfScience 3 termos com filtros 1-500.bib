@article{ WOS:000844118000005,
Author = {Gollub, Erica L. and Vaughan, Raven},
Title = {US WOMEN NEED THE DAPIVIRINE RING, TOO: FDA AS STRUCTURAL BARRIER TO HIV
   PREVENTION FOR WOMEN},
