@article{ WOS:001262782900001,
Author = {Hyacinthe, Marie-Fatima},
Title = {``These are our children and we got to set them free{''}: A public
   health approach to reading reproductive justice in black literature},
Journal = {JOURNAL OF LESBIAN STUDIES},
Year = {2024},
Month = {2024 JUN 22},
Abstract = {This paper explores reproductive justice themes in different works of
   Black literature and juxtaposes that literature with modern scholarship
   to consider a reproductive justice agenda for public health researchers.
   Incorporating multiple disciplines including public health, critical
   geography, and anthropology, this paper goes on to suggest that public
   health researchers would benefit from engagement with works from beyond
   academia. Specifically looking into Black fiction, nonfiction, and
   autobiographical writing, this paper traces reproductive justice themes
   and suggests that attention to these themes will bolster academic public
   health scholarship aligned with the reproductive justice movement},
