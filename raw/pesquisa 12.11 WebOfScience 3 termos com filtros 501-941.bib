@article{ WOS:001060103600001,
Author = {Alexander-Floyd, Nikol G.},
Title = {Psychoanalysis and Reproductive Justice: Reflections on Dobbs and the
   Possibilities of Psychoanalytic Political Praxis},
Journal = {CONTEMPORARY PSYCHOANALYSIS},
