@article{ WOS:000921127300003,
Author = {Rudrum, Sarah},
Title = {STUDENT ENCOUNTERS WITH A CAMPUS CRISIS PREGNANCY CENTRE: CHOICE, RE-
   PRODUCTIVE JUSTICE AND SEXUAL AND RE- PRODUCTIVE HEALTH SUPPORTS},
Journal = {CANADIAN JOURNAL OF SOCIOLOGY-CAHIERS CANADIENS DE SOCIOLOGIE},
Year = {2023},
Volume = {47},
Number = {1},
Pages = {53-74},
Abstract = {Taking a reproductive justice approach to understanding student needs, I
   explore student experiences of an on-campus crisis pregnancy centre
   (CPC), drawing on data from a small mixed methods study. Participants
   contacting the CPC sought testing, counselling, and referral to
   abortion; instead, they encoun-tered religious, anti-choice messages,
   and were left distressed and with delayed access to health care. These
   findings underscore the imperative that campuses provide accessible
   sexual and reproductive health services while simultaneously limiting
   campus access to anti-choice organizations.},
