@article{ WOS:000685845500005,
Author = {Scholz, Susanne},
Title = {DISMANTLING THE PHALLIC ECONOMY WITH A HERMENEUTICS OF REPRODUCTIVE
   JUSTICE A Reconsideration of the Sotah in Numbers 5:11-31},
Journal = {JOURNAL OF RELIGIOUS ETHICS},
Year = {2021},
Volume = {49},
Number = {2},
Pages = {270-289},
Month = {JUN},
Abstract = {A reproductive justice reading of Numbers 5:11-31 centers on ``the human
   right to maintain personal bodily autonomy, have children, not have
   children, and parent the children we have in safe and sustainable
   communities,{''} as the SisterSong Women of Color Reproductive Justice
   Collective stated so lucidly twenty years ago. Accordingly, a
   reproductive justice interpretation stresses the ethical imperative of
   reading Numbers 5:11-31 within the context of the reproductive justice
   movement. The interpretation centers on the most marginalized
   character-the woman-by recasting the woman's experience as sexual
   violence and not as adultery; it addresses the intersecting oppression
   as it is represented by the husband whose ally is the priest; it
   analyzes the depicted power system in Numbers 5:11-31; and it reimagines
   the dusty water drink as an herbal abortifacient.},
