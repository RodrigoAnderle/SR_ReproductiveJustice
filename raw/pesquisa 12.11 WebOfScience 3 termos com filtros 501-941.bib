@article{ WOS:000471418200001,
Author = {Gilmartin, Mary and McGing, Claire and Browne, Kath},
Title = {Feminist and gender geographies in Ireland},
Journal = {GENDER PLACE AND CULTURE},
Year = {2019},
Volume = {26},
Number = {7-9},
Pages = {1111-1118},
Month = {SEP 2},
Abstract = {This report discusses feminist and gender geographies in Ireland. We
   first focus on the ways in which gender constructs Irish geographies,
   updating numbers of women in academic positions across Ireland. This
   shows that women are increasingly in secure positions, but remain
   under-represented in more senior positions. We then turn to research. We
   discuss how femininities and women, and masculinities and men, have been
   addressed in Irish geographies. The focus on femininities and women is
   crucial given recent strides towards gender and reproductive justice. We
   then briefly summarise sexualities work. The report concludes by arguing
   that Ireland not only has vibrant gender/feminist geographical
   scholarship, it also has significant potential for emerging research and
   developing new theorisations and research agendas.},
