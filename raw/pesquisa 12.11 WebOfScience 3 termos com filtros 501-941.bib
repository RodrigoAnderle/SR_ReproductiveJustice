@article{ WOS:000210546700005,
Author = {Amery, Fran},
Title = {Intersectionality as Disarticulatory Practice: Sex-Selective Abortion
   and Reproductive Politics in the United Kingdom},
Journal = {NEW POLITICAL SCIENCE},
Year = {2015},
Volume = {37},
Number = {4},
Pages = {509-524},
Abstract = {Many authors have argued that sex-selective abortion (SSA) poses a
   problem for defenders of reproductive choice: the notion that a woman
   has ``freely chosen{''} to abort a female fetus becomes problematic when
   she faces compelling pressure to bear a male child. This argument
   reflects the broader concern of the reproductive justice movement that
   mainstream pro-choice discourse has defined ``choice{''} in narrow,
   legalistic terms, and overlooks the barriers to reproductive choice
   often faced by poor women and women of color. This article examines
   recent debates surrounding a proposed ban on SSA in the United Kingdom.
   It finds that despite attempts by the ban's proponents to make
   intersectional claims around gender, ethnicity, and class, their
   arguments also invoke xenophobia by constructing Indian migrants as a
   threat to ``British{''} values of gender equality. Thus, the article
   suggests that the concept of disarticulation may fruitfully be used to
   make sense of such ``intersectional{''} claims.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
