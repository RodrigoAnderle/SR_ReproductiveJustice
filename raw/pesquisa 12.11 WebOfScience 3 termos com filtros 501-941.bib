@article{ WOS:001227935000001,
Author = {Sealy-Jefferson, Shawnita and Jackson, Benita and Francis, Brittney},
Title = {Neighborhood eviction trajectories and odds of moderate and serious
   psychological distress during pregnancy among African American women},
