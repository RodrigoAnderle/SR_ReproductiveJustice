@article{ WOS:001345032500001,
Author = {Schmid, Spencer},
Title = {Why All US Medical Schools Have a Moral Obligation to Provide Abortion
   Training to Their Interested Students: A Necessary Response to
   <i>Dobbs</i>},
Journal = {INTERNATIONAL JOURNAL OF FEMINIST APPROACHES TO BIOETHICS},
Year = {2024},
Volume = {17},
Number = {2},
Pages = {85-113},
Month = {OCT 1},
Abstract = {Abortion is among the most widely disagreed upon topics in bioethics and
   healthcare. Consider how abortion is taught to medical students: while
   some medical schools incorporate abortion into their standard
   curriculum, others omit it entirely. In this article, the author argues
   these discrepancies go against society's interest in producing
   physicians with comprehensive medical knowledge-especially for common
   procedures like abortions. The author thus argues all US medical schools
   have a moral obligation to provide abortion education and clinical
   training opportunities to their students. For the sake of argumentation,
   the author attempts to justify this claim remaining morally neutral on
   abortion.},
