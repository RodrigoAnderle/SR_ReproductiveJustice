@article{ WOS:001058136400001,
Author = {Watson, Laurel B. and Hargons, Candice N. and Mollen, Debra},
Title = {Rising Up, Pushing Forward: Standing Our Ground in the Face of Injustice},
Journal = {PSYCHOLOGY OF WOMEN QUARTERLY},
