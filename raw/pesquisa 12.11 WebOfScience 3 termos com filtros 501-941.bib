
@article{ WOS:000625503500001,
Author = {Cheney, Kristen E.},
Title = {Discordant Expectations of Global Intimacy: Desire and Inequality in
   Commercial Surrogacy},
Journal = {SOCIOLOGICAL RESEARCH ONLINE},
Year = {2022},
Volume = {27},
Number = {1},
Pages = {43-59},
Month = {MAR},
Abstract = {Gestational surrogacy - carrying someone else's baby (or babies) to term
   and giving birth to them - is perhaps one of the most intimate acts a
   human being can perform for others. However, the proliferation of
   commercial surrogacy has drawn concern and criticism, with many scholars
   arguing that it both creates and exacerbates global social and economic
   inequalities. Commercial surrogacy thus raises both the possibility of
   global intimate connection and the specter of reproductive exploitation.
   I therefore explore the various, shifting, and often discordant desires
   for intimate connection between the intending parent(s), the surrogate
   mother, and the resulting child(ren) in commercial surrogacy. I then
   examine how those intimacies intersect with commercial surrogacy's
   socioeconomic inequalities. Weighing commercial surrogacy's driving
   desires and intimate practices against its commercialization, I end with
   a reconsideration of the procreative desires and intimate practices that
   spur current international commercial surrogacy (ICS), urging an
   emphasis on reproductive justice.},
