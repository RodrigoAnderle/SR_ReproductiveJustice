@article{ WOS:001214614300001,
Author = {Chiavaroli, Chiara},
Title = {Gendering Toxic Contamination: Toxic Risks, Bodies, and Pregnancies in
   Gold Mining and Coca Farming Communities in the Bajo Cauca Region},
Journal = {ANTIPODE},
Year = {2024},
Volume = {56},
Number = {5},
Pages = {1560-1580},
Month = {SEP},
Abstract = {This paper investigates women's everyday reproductive struggles in
   contexts of toxic contamination and the tensions emerging between toxic
   exposure and care in women's experiences of motherhood. While scientific
   framings of reproductive disruptions understand social identities as
   pre-existing the experience of toxic risks, in this paper I argue that,
   in toxic territories, the categories of ``contaminating{''} and
   ``contaminated{''} actors interact with other categories of identity,
   such as gender and race, shaping social relations. Drawing on 13 months
   of ethnographic fieldwork conducted in the Bajo Cauca region among gold
   mining and coca farming communities, I investigate the everyday
   processes of gendered subject formation that unfold in toxic territories
   and the emergence of ``faulty{''} gendered identities for rural mothers.
   Building on scholarship in feminist geography and Latin American
   feminist science and technology studies, I argue that ineffective forms
   of integration of gender in the institutional debate on toxic
   contamination reproduce, rather than challenge, the invisibility of
   rural women before the state.
   Este articulo analiza los desaf \& iacute;os reproductivos de las
   mujeres en contextos de contaminaci \& oacute;n t \& oacute;xica y las
   tensiones que emergen entre la exposici \& oacute;n t \& oacute;xica y
   las practicas de cuidado en sus experiencias de maternidad. Mientras los
   discursos cient \& iacute;ficos sobre contaminaci \& oacute;n
   conceptualizan las identidades sociales como preexistentes a la
   experiencia de riesgos t \& oacute;xicos, en este articulo argumento que
   las categor \& iacute;as de actores sociales ``contaminados{''} y ``que
   contaminan{''} interaccionan con otras categor \& iacute;as de identidad
   como el g \& eacute;nero y la raza, influenciando las relaciones
   sociales. Con base en 13 meses de trabajo de campo etnogr \& aacute;fico
   en la subregi \& oacute;n del Bajo Cauca con comunidades cocaleras y
   mineras, investigo los procesos cotidianos de formaci \& oacute;n de
   subjetividades de g \& eacute;nero que se desarrollan en territorios t
   \& oacute;xicos y la construcci \& oacute;n de una identidad
   ``defectuosa{''} impuesta a las madres rurales. El articulo contribuye a
   los debates en la geograf \& iacute;a feminista y los estudios
   feministas latinoamericanos de Ciencia y Tecnolog \& iacute;a,
   argumentando que las formas en las que el g \& eacute;nero ha sido
   integrado en el debate institucional sobre contaminaci \& oacute;n t \&
   oacute;xica reproduce, en vez de desafiar, la invisibilidad de las
   mujeres rurales frente al estado.},
