@article{ WOS:001153791400006,
Author = {Arguedas-Ramirez, Gabriela and Wenner, Danielle M.},
Title = {Reproductive Justice Beyond Borders: Global Feminist Solidarity in the
   Post-<i>Roe</i> Era},
Journal = {JOURNAL OF LAW MEDICINE \& ETHICS},
Year = {2023},
Volume = {51},
Number = {3, SI},
Pages = {606-611},
Abstract = {The global impact of Dobbs v. Jackson Women's Health Organization and
   the backlash towards reproductive justice that it represents warrant a
   global feminist response informed by broad theoretical and geopolitical
   lenses. We consider how a solidaristic, transnational feminist movement
   might learn from Latin American feminist movements that have been
   successful in uniting broad coalitions in the fight for reproductive
   justice as situated within far-reaching political goals. The success of
